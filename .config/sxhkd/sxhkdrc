# scripts and other commands
Print
	maim $HOME/pix/screenshots/$(date +%y%m%d-%H%m-%S).jpg
super + BackSpace
	sysact
super + shift + n
	notify-send "mpc" "$(mpc current)"
super + b
	add-bookmark
super + i
	xdotool type $(grep -v '^#' ~/.local/share/bookmarks | dmenu -i -l 50 | cut -d' ' -f1)
super + shift + i
	dmenupass --copy

# Run programs
super + w
	$BROWSER
super + shift + w
	firefox
super + v
	$TERMINAL $EDITOR
super + e
	$TERMINAL neomutt
super + shift + e
	dmenuunicode
super + y
	$TERMINAL ytfzf -t
super + n
	$TERMINAL ncmpcpp
super + shift + n
	$TERMINAL nmtui
super + r
	$TERMINAL ranger
super + shift + t
	torbrowser-launcher

# mpc and pulseaudio
super + p
	mpc toggle
super + comma
	mpc seek -10
super + period
	mpc seek +10
super + shift + comma
	mpc prev
super + shift + period
	mpc next
super + m
	pamixer --toggle-mute
super + =
	pamixer -i 5
super + +
	pamixser -i 10

# F keys
super + F1
	mpv --untimed --no-cache --no-osc --no-input-default-bindings --profile=low-latency --input-conf=/dev/null --title=webcam $(ls /dev/video[0,2,4,6,8] | tail -n 1)

super + F2
	pulsemixer
